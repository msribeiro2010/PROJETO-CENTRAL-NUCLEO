{
  "name": "central-nucleo-server",
  "version": "1.0.0",
  "description": "Server for Central Nucleo application with PJe Database integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "npm install && echo Dependencias instaladas com sucesso!",
    "open": "node -e \"require('child_process').exec(process.platform === 'win32' ? 'start http://localhost:3000' : process.platform === 'darwin' ? 'open http://localhost:3000' : 'xdg-open http://localhost:3000')\"",
    "go": "npm run setup && npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  }
} 